
<script type="text/javascript">
$(document).ready(function() {
    $("body").tooltip({ selector: '[data-toggle=tooltip]' });
});
</script>

<div class="rate"data-toggle="tooltip" title="Rate of cost per hour" data-placement="bottom">Rate: 
	{{#each rate}}
		${{this.value}}/Hour
	{{/each}}
</div>


<div class="usage"data-toggle="tooltip" title="Current usage from specified start/end date" data-placement="bottom">Credits Used:
	{{#each usage}}
		${{this.value}}
	{{/each}}
</div>

<div class="balance" data-toggle="tooltip" title="Current remaining balance" data-placement="bottom">Balance:
	{{#each balance}}
	 	${{this.value}}
	{{/each}}
</div>

